PROMPT Creating Table 'WORK_ORDER_CLAIMS'
CREATE TABLE WORK_ORDER_CLAIMS
 (WOC_CLAIM_REF VARCHAR2(20) NOT NULL
 ,WOC_CON_ID NUMBER(9,0) NOT NULL
 ,WOC_CLAIM_TYPE VARCHAR2(1) NOT NULL
 ,WOC_CLAIM_DATE DATE NOT NULL
 ,WOC_CLAIM_VALUE NUMBER(11,2) NOT NULL
 ,WOC_WORKS_ORDER_NO VARCHAR2(16) NOT NULL
 ,WOC_INTERIM_NO NUMBER(3,0)
 )
/

COMMENT ON TABLE WORK_ORDER_CLAIMS IS 'Claim/Invoice details. Populated by financial and contractual interface and by a db trigger for manual orders.'
/

PROMPT Creating Table 'WORK_ORDER_LINES'
CREATE TABLE WORK_ORDER_LINES
 (WOL_ID NUMBER(9,0) NOT NULL
 ,WOL_WORKS_ORDER_NO VARCHAR2(16) NOT NULL
 ,WOL_RSE_HE_ID NUMBER(9,0) NOT NULL
 ,WOL_SISS_ID VARCHAR2(3) NOT NULL
 ,WOL_ICB_WORK_CODE VARCHAR2(10) NOT NULL
 ,WOL_DEF_DEFECT_ID NUMBER(8,0)
 ,WOL_REP_ACTION_CAT VARCHAR2(1)
 ,WOL_SCHD_ID NUMBER(9,0)
 ,WOL_CNP_ID NUMBER(9,0)
 ,WOL_ACT_AREA_CODE VARCHAR2(2)
 ,WOL_ACT_COST NUMBER(10,2)
 ,WOL_ACT_LABOUR NUMBER(10,2)
 ,WOL_ARE_END_CHAIN NUMBER(6,0)
 ,WOL_ARE_REPORT_ID NUMBER(8,0)
 ,WOL_ARE_ST_CHAIN NUMBER(6,0)
 ,WOL_CHECK_CODE VARCHAR2(4)
 ,WOL_CHECK_COMMENTS VARCHAR2(254)
 ,WOL_CHECK_DATE DATE
 ,WOL_CHECK_ID NUMBER(9,0)
 ,WOL_CHECK_PEO_ID NUMBER(8,0)
 ,WOL_CHECK_RESULT VARCHAR2(1)
 ,WOL_DATE_COMPLETE DATE
 ,WOL_DATE_CREATED DATE
 ,WOL_DATE_PAID DATE
 ,WOL_DESCR VARCHAR2(1000)
 ,WOL_DISCOUNT NUMBER(6,3)
 ,WOL_EST_COST NUMBER(10,2)
 ,WOL_EST_LABOUR NUMBER(10,2)
 ,WOL_FLAG VARCHAR2(1)
 ,WOL_MONTH_DUE DATE
 ,WOL_ORIG_EST NUMBER(10,2)
 ,WOL_PAYMENT_CODE VARCHAR2(20)
 ,WOL_QUANTITY NUMBER(10,2)
 ,WOL_RATE NUMBER(10,2)
 ,WOL_SS_TRE_TREAT_CODE VARCHAR2(4)
 ,WOL_STATUS VARCHAR2(1)
 ,WOL_STATUS_CODE VARCHAR2(10)
 ,WOL_UNIQUE_FLAG VARCHAR2(1)
 ,WOL_WORK_SHEET_DATE DATE
 ,WOL_WORK_SHEET_ISSUE NUMBER(2,0)
 ,WOL_WORK_SHEET_NO NUMBER(6,0)
 ,WOL_WOR_FLAG VARCHAR2(1)
 ,WOL_DATE_REPAIRED DATE
 ,WOL_INVOICE_STATUS VARCHAR2(1)
 ,WOL_BUD_ID NUMBER(9)
 ,WOL_UNPOSTED_EST NUMBER(10,2)
 ,WOL_IIT_ITEM_ID NUMBER(9)
 ,WOL_GANG NUMBER
 ,WOL_FINAL VARCHAR2(1)
 ,WOL_REGISTER_FLAG VARCHAR2(1) DEFAULT 'N' NOT NULL
 ,WOL_LOCN_DESCR VARCHAR2(1000)
 ,WOL_TARGET_DATE DATE
 ,WOL_BOQ_PERC_ITEM_CODE VARCHAR2(10)
 ,WOL_WOL_PERC_ITEM_CODE VARCHAR2(10)
 )
/

COMMENT ON TABLE WORK_ORDER_LINES IS 'Details of lines within a work order.  Each line corresponds to a job on a single road section.'
/

